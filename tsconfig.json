{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "react",
    "reactNamespace": "app",
    "lib": ["dom", "es2015", "es5"],
    "experimentalDecorators": true,
    "sourceMap": true,

    "baseUrl": "./",
    "paths": {
      "~charts/*": ["src/charts/*"],
      "~components/*": ["src/components/*"],
      "~data/*": ["src/data/*"],
      "~pages/*": ["src/pages/*"],
      "~router/*": ["src/router/*"],
      "~styles/*": ["src/styles/*"]
    }
  }
}