{
  "name": "bc_visualization",
  "version": "0.0.1",
  "description": "Skeleton website for visualization work",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "analyze": "webpack-bundle-analyzer stats.json dist/",
    "clean": "npm run build:clean",
    "build": "npm run build:website",
    "build:clean": "npm run build:clean:website && npm run build:clean:data",
    "build:clean:data": "rimraf dist_data/ && rimraf src/data/",
    "build:clean:website": "rimraf dist/",
    "build:website": "npm run build:clean:website && webpack --mode production --profile --json > stats.json && npm run lint",
    "convert:data:csv:to:json": "make-dir dist_data && foreach -g \"data/*.csv\" -x \"csvtojson --trim=true #{path} > dist_data/#{name}.json\"",
    "convert:data:csv:to:ts": "npm run convert:data:csv:to:json && npm run convert:data:json:to:ts",
    "convert:data:json:to:ts": "make-dir src/data && foreach -g \"dist_data/*.json\" -x \"echo \\\"export const data: any[] =\\\" > src/data/#{name}.ts && cat #{path} >> src/data/#{name}.ts && echo \\\";\\\" >> src/data/#{name}.ts\" && npm run lint:fix src/data/*.ts",
    "deploy": "echo \"Nowhere to deploy yet",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^3.2.0",
    "cssnano-preset-advanced": "^4.0.7",
    "csvtojson": "^2.0.10",
    "file-loader": "^4.2.0",
    "foreach-cli": "^1.8.1",
    "glob-all": "^3.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "license-webpack-plugin": "^2.1.2",
    "make-dir-cli": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "purgecss-webpack-plugin": "^1.5.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "sitemap": "^4.1.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "terser": "^4.2.0",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "d3": "^5.11.0",
    "lodash.clonedeep": "^4.5.0"
  }
}
